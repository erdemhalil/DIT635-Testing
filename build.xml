<?xml version="1.0" encoding="UTF-8"?>
<project name="Testing" basedir="." default="all">
	<property name="src.dir" value="src" />
	<property name="build.dir" value="CoffeeMaker/bin/main/java" />
	<path id="classpath">
		<fileset dir="${src.dir}/main">
			<include name="*.java" />
		</fileset>
		<pathelement path="${build.dir}" />
	</path>
    <target name = "all" depends = "clean">
    <mkdir dir="${build.dir}"/></target>
	<target name="clean">
		<delete>
			<fileset dir="${build.dir}">
				<include name="**/*.class" />
			</fileset>
		</delete>
	</target>
	<target name="Compile">
	</target>
</project>
